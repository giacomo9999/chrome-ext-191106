{
  "manifest_version": 2,
  "name": "Highlighter",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js", "index.js"],
      "css": ["styles.css"]
    }
  ],
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
  "browser_action": {
    "default_icon": "magic_icon.png",
    "default_popup": "index.html",
    "default_title": "My First Chrome Extension"
  }
}
